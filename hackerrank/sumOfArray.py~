from array import array

class Solution:
    def __init__(self, n):
        self.n = n
        self.ar = array('i')

    def sumOfArray(self):
        value = input().split()
        for i in range(self.n):
            self.ar.append(int(value[i]))
        return sum(self.ar)

n = int(input())
solution = Solution(n)
print(solution.sumOfArray())

